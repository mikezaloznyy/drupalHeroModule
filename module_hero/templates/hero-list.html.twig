{{ attach_library('module_hero/hero-style') }}
{% set effects = [
    'bounce',
    'flash',
    'rubberBand',
    'shake',
    'jello',
    'tada',
    'bounceIn',
    'rollIn'
    ]
%}
<h4 class="animated bounce">{{ title }}</h4>
<ol>
    {% for item in items %}
        <li class="animated {{ random(effects) }}">{{ item.name }}</li>
    {% endfor %}
</ol>
<strong>Coming from twig file</strong>